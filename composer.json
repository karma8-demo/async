{
    "name": "karma8demo/async",
    "type": "project",
    "require": {
        "php": "^8.2",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "ext-phar": "*",
        "ext-zend-opcache": "*",
        "amphp/amp": "*",
        "amphp/mysql": "*",
        "amphp/parallel": "*",
        "symfony/dotenv": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "discard-changes": true,
        "htaccess-protect": false,
        "notify-on-install": false,
        "optimize-autoloader": true,
        "platform-check": false,
        "sort-packages": true
    }
}
